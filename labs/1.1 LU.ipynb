    "def method_Gaussa(a):\n",
    "    n = len(a)\n",
    "    z = [[0]*n for i in range(n)]\n",
    "    x = [0]*n\n",
    "\n",
    "    for i in range(0, n):\n",
    "        for j in range(0, n):\n",
    "            if i == j:\n",
    "                z[i][j] = 1\n",
    "            else:\n",
    "                z[i][j] = 0\n",
    "    p = 0\n",
    "\n",
    "    #Для метода Гаусса с выбором ведущего элемента\n",
    "    for k in range(0, n-1):      \n",
    "        if a[k][k] > 0:\n",
    "            g = a[k][k]\n",
    "        else:\n",
    "            g = -a[k][k]\n",
    "        m = k\n",
    "\n",
    "        for i in range(k + 1, n):\n",
    "            if a[i][k] > 0:\n",
    "                f = a[i][k]\n",
    "            else:\n",
    "                f = -a[i][k]\n",
    "            if f > g:\n",
    "                g = f\n",
    "                m = i\n",
    "        if g == 0:\n",
    "            print(\"Решение не единственно или отсутствует!\")\n",
    "\n",
    "        if m != k:\n",
    "            for j in range(0, n):\n",
    "                g = a[k][j]\n",
    "                a[k][j] = a[m][j]\n",
    "                a[m][j] = g\n",
    "            g = b[k]\n",
    "            b[k] = b[m]\n",
    "            b[m] = g\n",
    "\n",
    "            for j in range(0, n):\n",
    "                g = z[k][j]\n",
    "                z[k][j] = z[m][j]\n",
    "                z[m][j] = g\n",
    "            p += 1\n",
    "\n",
    "        #Общая часть метода Гаусса\n",
    "        for i in range(k + 1, n):\n",
    "            h = -a[i][k]/a[k][k]\n",
    "            for j in range(k, n):\n",
    "                a[i][j] = a[i][j] + h * a[k][j]\n",
    "            b[i] = b[i] + h * b[k]\n",
    "\n",
    "            for j in range(0, n):\n",
    "                z[i][j] = z[i][j] + h * z[k][j]\n",
    "        \n",
    "    y = 1 - 2 * (p % 2)\n",
    "\n",
    "    # Определитель\n",
    "    for k in range(0, n):\n",
    "        y = y * a[k][k]\n",
    "\n",
    "    #Обратный ход\n",
    "    for k in range(n - 1, -1, -1):\n",
    "        h = 0\n",
    "        for i in range(k + 1, n):\n",
    "            h = h + a[k][i] * x[i]\n",
    "        x[k] = (b[k] - h) / a[k][k]\n",
    "\n",
    "        for j in range(0, n):\n",
    "            h = 0\n",
    "            for i in range(k + 1, n):\n",
    "                h = h + a[k][i] * z[i][j]\n",
    "            z[k][j] = (z[k][j] - h) / a[k][k]\n",
    "\n",
    "    return x, y, z\n",
    "\n",
    "def get_LU(A):\n",
    "    n = len(A)\n",
    "    L = [[0] * n for i in range(0, n)]\n",
    "    U = [[0] * n for i in range(0, n)]\n",
    "\n",
    "    for i in range(n):\n",
    "        L[i][i] = 1\n",
    "        for j in range(n):\n",
    "            if j < i:\n",
    "                L[i][j] = a[i][j]\n",
    "            else:\n",
    "                U[i][j] = a[i][j]\n",
    "    return L, U\n",
    "\n",
    "a = [[1, -5, -7, 1], [1, -3, -9, -4], [-2, 4, 2, 1], [-9, 9, 5, 3]]\n",
    "b = [-75, -41, 18, 29]\n",
    "\n",
    "x, y, z = method_Gaussa(a)\n",
    "print(\"Метода Гаусса с выбором главного элемента: \")\n",
    "print(\"Решение СЛАУ: \")\n",
    "print(\"x = (\", *x, \")\")\n",
    "\n",
    "print(\"Определитель: \")\n",
    "print(\"det =\", y)\n",
    "print()\n",
    "\n",
    "print(\"Обратная матрица: \")\n",
    "print(\"A(-1) = \", z)\n",
    "print()\n",
    "\n",
    "L, U = get_LU(a)\n",
    "print(\"L:\")\n",
    "print(L)\n",
    "print()\n",
    "\n",
    "print(\"U:\")\n",
    "print(U)\n",
    "print()"
